{
  "name": "vs-gpt-chat",
  "displayName": "vs-gpt-chat",
  "description": "A basic extension to use LLM models in VS Code",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vs-gpt-chat.startChat",
        "title": "DY Start Chat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "install:all": "pnpm install && cd webview-ui && pnpm install",
    "start:webview": "cd webview-ui && pnpm run start",
    "watch:webview": "cd webview-ui && pnpm run watch",
    "build:webview": "cd webview-ui && pnpm run build",
    "db:seed": "dotenvx run -- ts-node ./src/scripts/seedDb.ts"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.35.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@datastax/astra-db-ts": "^1.5.0",
    "@langchain/community": "^0.3.30",
    "@langchain/core": "0.3.39",
    "@langchain/langgraph": "^0.2.46",
    "@langchain/ollama": "^0.2.0",
    "langchain": "^0.3.15",
    "ollama": "^0.5.12",
    "puppeteer": "^24.2.0"
  },
  "overrides": {
    "@langchain/core": "0.3.39"
  },
  "pnpm": {
    "overrides": {
      "@langchain/core": "0.3.39"
    }
  }
}
